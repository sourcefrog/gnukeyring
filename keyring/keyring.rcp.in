		AUTOID AT (CENTER PREVBOTTOM) FONT 1

  	LABEL "Copyright 1999, 2000 by Martin Pool" AUTOID AT (CENTER PREVBOTTOM+4)
	LABEL "This is free software.  No warranty." AUTOID AT (CENTER PREVBOTTOM) 
	LABEL "Released under the GNU GPL v2." AUTOID AT (CENTER PREVBOTTOM) 

	LABEL "http://gnukeyring.sourceforge.net/" AUTOID AT (CENTER PREVBOTTOM+2)

	BUTTON "OK" AUTOID AT (CENTER 139 30 12)
END



MENU ID ListMenuBar
BEGIN
	PULLDOWN "Keyring"
	BEGIN
		MENUITEM "Set Password..." ID SetPasswdCmd 
	END

	PULLDOWN "Options"
	BEGIN
		MENUITEM "Preferences..." ID ID_PrefsCmd
		MENUITEM "About..." ID AboutCmd
	END
END


MENU ID KeyEditMenuBar
BEGIN
	PULLDOWN "Key"
	BEGIN
		MENUITEM "Generate Password..." ID GenerateCmd
		MENUITEM "Export to Memo" ID ExportMemoCmd
		MENUITEM SEPARATOR
		MENUITEM "Undo All" ID ID_UndoAllCmd
		MENUITEM "Delete..." id DeleteKeyCmd 
	END

	PULLDOWN "Edit"
	BEGIN
		MENUITEM "Undo" ID EditUndo "@accel_Undo"
		MENUITEM "Cut" ID EditCut "@accel_Cut"
		MENUITEM "Copy" ID EditCopy "@accel_Copy"
		MENUITEM "Paste" ID EditPaste "@accel_Paste"
		MENUITEM "Select All" ID EditSelectAll "@accel_SelectAll"
		MENUITEM SEPARATOR
		MENUITEM "Keyboard" ID KeyboardCmd "@accel_Keyboard"
		MENUITEM "Graffiti Help" ID GraffitiReferenceCmd "@accel_Graffiti"
	END

	PULLDOWN "Options"
	BEGIN
		MENUITEM "Preferences..." ID ID_PrefsCmd 
		MENUITEM "Tips..."	ID HelpCmd
		MENUITEM "About..." ID AboutCmd
	END
END

