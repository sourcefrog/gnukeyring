VERSION ID 1000 "__VERSION__"

FONT ID StarFont FONTID fntStar "fontstar.txt"
FONT ID PasswordFont FONTID fntPassword "fontPW.txt"

FORM ID ListForm AT (0 0 160 160)
MENUID ListMenuBar
BEGIN
	TITLE "Keyring"

	BUTTON "" ID LockBtn AT (RIGHT@160 1 10 11) NOFRAME
	POPUPTRIGGER "" ID CategoryTrigger AT (RIGHT@149 0 AUTO AUTO) RIGHTANCHOR USABLE
	LIST "" ID CategoryList AT (RIGHT@159 0 80 AUTO) NONUSABLE VISIBLEITEMS 1

	TABLE ID ID_KeyTable AT (0 15 152 132) COLUMNS 1 ROWS 12
		EDITABLE COLUMNWIDTHS 150

	SCROLLBAR ID ID_KeyTableScrollBar 
		AT (PREVRIGHT+1 PREVTOP 7 PREVHEIGHT)
		VALUE 0 MIN 0 MAX 0 PAGESIZE 0

	LABEL "Look Up:" AUTOID AT (0 149)
	FIELD ID LookUpFld AT (PREVRIGHT+2 PREVTOP 70 AUTO) UNDERLINED MAXCHARS 20
	BUTTON "New" NewKeyBtn AT (RIGHT@149 148 AUTO 11)
	GRAFFITISTATEINDICATOR AT (152 150)
END


FORM ID KeyEditForm AT (0 0 160 160)
MENUID KeyEditMenuBar
BEGIN
	/* The default title buffer must be big enough for anything we 
	 * want to copy into it */
	TITLE "Key ______ of ______" 

	FORMBITMAP AT (151 1) BITMAP UnlockBitmap
	BUTTON "" ID LockBtn AT (RIGHT@160 1 10 11) NOFRAME
	POPUPTRIGGER "" ID CategoryTrigger AT (RIGHT@149 1 AUTO AUTO) 
		RIGHTANCHOR USABLE

	LIST "" ID CategoryList AT (RIGHT@159 1 80 AUTO) NONUSABLE VISIBLEITEMS 1

	FIELD ID ID_KeyNameField AT (47 16 110 AUTO) USABLE  
		UNDERLINED AUTOSHIFT MAXCHARS 40

	LABEL "Name:" AUTOID AT (RIGHT@PREVLEFT-2 PREVTOP) 

	FIELD ID AccountField AT (47 PREVBOTTOM 110 AUTO) USABLE
		UNDERLINED MAXCHARS 50

	LABEL "Account:" AUTOID AT (RIGHT@PREVLEFT-2 PREVTOP) 

	FIELD PasswordField AT (47 PREVBOTTOM 110 AUTO) USABLE 
		UNDERLINED MAXCHARS 50

	LABEL "Password:" AUTOID AT (RIGHT@PREVLEFT-2 PREVTOP) 

	SELECTORTRIGGER "1/4/99" ID DateTrigger AT (47 PREVBOTTOM+1 40 AUTO)
		LEFTANCHOR

        LABEL "Changed:" AUTOID AT (RIGHT@PREVLEFT-2 PREVTOP)

	FIELD ID ID_NotesField AT (0 PREVBOTTOM+2 152 77) USABLE
		UNDERLINED AUTOSHIFT MULTIPLELINES MAXCHARS 32760 
	SCROLLBAR ID NotesScrollbar AT (152 PREVTOP 7 PREVHEIGHT) USABLE 
	
	BUTTON "Done" DoneBtn AT (1 147 AUTO AUTO)
	BUTTON "Generate" GenerateBtn AT (PREVRIGHT+5 PREVTOP AUTO AUTO)

	GRAFFITISTATEINDICATOR AT (152 150)
END


FORM ID ID_PrefsForm AT (2 96 156 62)
MODAL
BEGIN
	TITLE "Preferences"

	LABEL "Stay unlocked for:" AUTOID AT (5 15) FONT 1

	PUSHBUTTON "No time" ID Expiry0Push AT (5 PREVBOTTOM+1 AUTO AUTO)
		GROUP 1
	PUSHBUTTON "15 sec" ID Expiry15Push AT (PREVRIGHT+1 PREVTOP AUTO AUTO)
		GROUP 1
	PUSHBUTTON "60 sec" ID Expiry60Push AT (PREVRIGHT+1 PREVTOP AUTO AUTO)
		GROUP 1
	PUSHBUTTON "5 min" ID Expiry300Push AT (PREVRIGHT+1 PREVTOP AUTO AUTO)
		GROUP 1

/*        CHECKBOX "Lock when switched off" ID ID_PowerOffLock 
               AT (3 PREVBOTTOM+5 AUTO AUTO) FONT 1
*/
	
	BUTTON "OK" OkBtn AT (5 PREVBOTTOM+6 AUTO AUTO) 
	BUTTON "Cancel" CancelBtn AT (PREVRIGHT+6 PREVTOP AUTO AUTO)
END


FORM ID SetPasswdForm AT (2 78 156 80)
HELPID SetPasswdHelp
MODAL
BEGIN
	title "Set Keyring Password"

	label "@@choosepassword"
		autoid at (5 15) font 0

	label "Password:" 
		autoid at (prevleft PREVBOTTOM+2) font 1

	field id MasterKeyFld at (65 prevtop 85 15)
		underlined maxchars 40

	label "Confirm:" 
		autoid at (5 prevbottom) font 1

	field id ConfirmFld at (65 prevtop 85 15)
		underlined maxchars 40

	button "OK" OkBtn at (5 prevbottom+4 auto auto)
	button "Cancel" CancelBtn at (prevright+5 prevtop auto auto)

	graffitistateindicator at (140 prevtop)
end


FORM ID GenerateForm AT (2 50 156 108)
HELPID GenerateHelp
MODAL
BEGIN
	TITLE "Generate Password"

	LABEL "@@generate1" AUTOID AT (3 15) FONT 1
	LABEL "@@generate2" AUTOID AT (PREVLEFT PREVBOTTOM) FONT 1

	LABEL "Length:" autoid at (right@43 prevbottom+4) font 1
	pushbutton "4" id Length4Push at (46 prevtop auto auto)		  group 1
	pushbutton "6" id Length6Push at (prevright+1 prevtop auto auto)  group 1
	pushbutton "8" id Length8Push at (prevright+1 prevtop auto auto)  group 1 
	pushbutton "10" id Length10Push at (prevright+1 prevtop auto auto) group 1
	pushbutton "16" id Length16Push at (prevright+1 prevtop auto auto) group 1
	pushbutton "20" id Length20Push at (prevright+1 prevtop auto auto) group 1
	
	LABEL "Include:" AUTOID AT (RIGHT@43 PREVBOTTOM+6) FONT 1
	PUSHBUTTON "a-z" ID IncludeLower at (46 prevtop auto auto) 
	PUSHBUTTON "A-Z" id IncludeUpper at (prevright+1 prevtop auto auto) 
	PUSHBUTTON "0-9" id IncludeDigits at (prevright+1 prevtop auto auto) 
	PUSHBUTTON "!@$" id IncludePunct at (prevright+1 prevtop 20 auto)
        PUSHBUTTON "@@langSpecificChars" ID ID_IncludeHigh AT (prevright+1 prevtop 22 AUTO)
        PUSHBUTTON "@@pronounceable" ID ID_Syllables AT (46 PREVBOTTOM+1 PREVRIGHT-46 AUTO)

	BUTTON "Generate" id OkBtn at (3 PREVBOTTOM+7 auto auto)
	button "Cancel" id CancelBtn at (prevright+5 prevtop auto auto)
end


FORM ID UnlockForm AT (2 86 156 72)
MODAL
BEGIN
	TITLE "Unlock"
	
	LABEL "Enter your password:" AUTOID AT (3 15) FONT 1

	FIELD ID MasterKeyFld AT (3 prevbottom+1 150 auto)
