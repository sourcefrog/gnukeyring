	MESSAGE "This database is from a previous version of Keyring.  Convert it to the new format?  (Please make a backup before doing this.)"

	BUTTONS "Convert" "Cancel"
END


ALERT ID BetaAlert
WARNING
BEGIN
  TITLE "Beta Software"

  MESSAGE "@@BetaVersion"

  BUTTONS "OK" 
END


ALERT ID CantUpgradeAlert
ERROR
BEGIN
	TITLE "Error"

	MESSAGE "Can't upgrade database from version ^1."

	BUTTONS "OK"
END


ALERT ID UpgradeFailedAlert
ERROR
BEGIN
	TITLE "Error"

	MESSAGE "Error upgrading database: ^1"

	BUTTONS "OK"
END


ALERT ID alertID_PasswordHashMissing
WARNING
BEGIN
    TITLE "Database Damaged"

    MESSAGE "This database has been damaged.  Please enter your password to repair it. If you mistype the password your database will be broken so make a backup first."

    BUTTONS "OK" "Cancel"
END


ALERT ID ID_CategoriesMissing
WARNING
BEGIN
	TITLE "Database Upgrade"

	MESSAGE "There are no categories defined in this database.  Default categories have been added."

	BUTTONS "OK"
END


ALERT ID NotEnoughFeaturesAlert
ERROR
BEGIN
	TITLE "Not Enough Features"

	MESSAGE "This version of Keyring requires PalmOS 3.0 and the DES encryption feature (MS-Chap support).  It cannot run here."

	BUTTONS "OK"
END


ALERT ID ID_SortError
ERROR
BEGIN	
	TITLE "Sort Error"

	MESSAGE "Error sorting database: ^1"

	BUTTONS "OK"
END


ALERT ID TooNewAlert
ERROR
BEGIN
	TITLE "Incompatible Database"

	MESSAGE "This database is from a later version of Keyring, and cannot be opened."

	BUTTONS "Cancel"
END


ALERT ID alertID_UpgradeReadOnly
ERROR
BEGIN
	TITLE "Incompatible Database"

	MESSAGE "This database is from an earlier version of Keyring, but cannot be converted because it is in read-only memory."
	
	BUTTONS "Cancel"
END


FORM ID ConfirmDeleteForm AT (2 94 156 64)
MODAL
BEGIN
	TITLE "Delete Key"

	LABEL "Delete current key record?" AUTOID AT (5 16) FONT 1 

	CHECKBOX "Save archive copy on PC" ID SaveArchiveCheck 
		AT (5 PREVBOTTOM + 3 AUTO AUTO) FONT 1

	BUTTON "OK" ID OkBtn AT (5 46 AUTO AUTO) 
	BUTTON "Cancel" ID CancelBtn AT (PREVRIGHT + 5 PREVTOP AUTO AUTO)
END



FORM ID AboutForm AT (2 2 156 156)
MODAL
BEGIN
	TITLE "Keyring for PalmOS"

	FORMBITMAP AT (69 20) BITMAP KeyBitmap

	LABEL "Keyring"
		AUTOID AT (CENTER 50) FONT 1
	LABEL "__VERSION__ (__DATE__)" 
		AUTOID AT (CENTER PREVBOTTOM) FONT 1

  	LABEL "Copyright 1999-2002 by Martin Pool"
		AUTOID AT (CENTER PREVBOTTOM+4)
	LABEL "This is free software.  No warranty." AUTOID AT (CENTER PREVBOTTOM) 
	LABEL "Released under the GNU GPL v2." AUTOID AT (CENTER PREVBOTTOM) 

	LABEL "http://gnukeyring.sourceforge.net/" AUTOID AT (CENTER PREVBOTTOM+2)

	BUTTON "OK" AUTOID AT (CENTER 139 30 12)
END



MENU ID ListMenuBar
BEGIN
	PULLDOWN "Keyring"
	BEGIN
		MENUITEM "Set Password..." ID SetPasswdCmd 
	END

	PULLDOWN "Options"
	BEGIN
		MENUITEM "Preferences..." ID ID_PrefsCmd
		MENUITEM "About..." ID AboutCmd
	END
END


MENU ID KeyEditMenuBar
BEGIN
	PULLDOWN "Key"
	BEGIN
		MENUITEM "Generate Password..." ID GenerateCmd
		MENUITEM "Export to Memo" ID ExportMemoCmd
		MENUITEM SEPARATOR
		MENUITEM "Undo All" ID ID_UndoAllCmd
		MENUITEM "Delete..." id DeleteKeyCmd 
	END

	PULLDOWN "Edit"
	BEGIN
		MENUITEM "Undo" ID 10000 "@@accel_Undo"
		MENUITEM "Cut" ID 10001 "@@accel_Cut"
		MENUITEM "Copy" ID 10002 "@@accel_Copy"
		MENUITEM "Paste" ID 10003 "@@accel_Paste"
		MENUITEM "Select All" ID 10004 "@@accel_SelectAll"
		MENUITEM SEPARATOR
		MENUITEM "Keyboard" ID 10006 "@@accel_Keyboard"
		MENUITEM "Graffiti Help" ID 10007 "@@accel_Graffiti"
	END

	PULLDOWN "Options"
	BEGIN
		MENUITEM "Preferences..." ID ID_PrefsCmd 
		MENUITEM "Tips..."	ID HelpCmd
		MENUITEM "About..." ID AboutCmd
	END
END

